<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_stock_move_line_operation_tree" model="ir.ui.view">
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_stock_move_line_operation_tree" />
        <field name="arch" type="xml">
            <field name="qty_done" position="before">
                <field name="is_repair" invisible="1"/>
            </field>
            <field name="qty_done" position="attributes">
                <attribute name="attrs">{'invisible': [('is_repair', '=', True)], 'readonly': ['|', '&amp;', ('state', '=', 'done'), ('is_locked', '=', True), '&amp;', ('package_level_id', '!=', False), ('parent.picking_type_entire_packs', '=', True)]}</attribute>
            </field>
        </field>
    </record>
</odoo>
