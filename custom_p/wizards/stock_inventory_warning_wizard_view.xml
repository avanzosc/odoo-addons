<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="stock_inventory_warning_wizard_view_form" model="ir.ui.view">
        <field name="name">stock.inventory.warning.wizard.form</field>
        <field name="model">stock.inventory.warning.wizard</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="text" readonly="1" nolabel="1" />
                    </group>
                    <footer>
                        <button
              name="continue_with_inventory"
              string="Accept"
              type="object"
              class="oe_link"
            />
                        or
                        <button special="cancel" string="Cancel" class="oe_highlight" />
                    </footer>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
