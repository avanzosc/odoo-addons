<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="sale_order_line_price_history_view_form" model="ir.ui.view">
        <field name="model">sale.order.line.price.history</field>
        <field
      name="inherit_id"
      ref="sale_order_line_price_history.sale_order_line_price_history_view_form"
    />
        <field name="arch" type="xml">
            <xpath expr="//tree/field[@name='sale_order_date_order']" position="after">
                <field name="commitment_date" />
                <field name="lot_id" />
            </xpath>
        </field>
    </record>
</odoo>
