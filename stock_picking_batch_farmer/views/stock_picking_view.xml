<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_picking_form" model="ir.ui.view">
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form" />
        <field name="arch" type="xml">
            <field name="picking_type_id" position="after">
                <field name="batch_location_id" invisible="1" />
            </field>
            <field name="picking_type_id" position="attributes">
                <attribute
          name="domain"
        >['|', ('default_location_src_id','=', batch_location_id), ('default_location_dest_id', '=', batch_location_id), '|', ('default_location_src_id', '!=', location_change_id), ('default_location_src_id', '=', False)]</attribute>
            </field>
        </field>
    </record>

</odoo>
