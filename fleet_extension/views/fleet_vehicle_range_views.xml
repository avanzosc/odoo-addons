         <field name="domain">[]</field>
         <field name="context">{}</field>
    </record>

    <menuitem
    id="menu_fleet_vehicle_range"
    action="action_fleet_vehicle_range"
    name="Vehicle Range"
    parent="fleet.fleet_configuration"
