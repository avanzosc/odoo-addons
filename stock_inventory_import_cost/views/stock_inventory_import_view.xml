<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="stock_inventory_import_view_form" model="ir.ui.view">
        <field name="model">stock.inventory.import</field>
        <field
      name="inherit_id"
      ref="stock_inventory_import_wizard.stock_inventory_import_view_form"
    />
        <field name="arch" type="xml">
            <xpath expr="//page[@name='help_page']/group/div/ul" position="inside">
                <li><code>Precio</code>: this will fill <b
          >Inventory Product Cost</b></li>
                <li><code>Importe</code>: this will fill <b
          >Inventory Product Amount</b></li>
            </xpath>
        </field>
    </record>
</odoo>
