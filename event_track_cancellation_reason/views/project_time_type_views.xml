<?xml version="1.0" encoding="utf-8"?>

<odoo>

    <record id="view_project_time_type_form" model="ir.ui.view">
        <field name="model">project.time.type</field>
        <field name="inherit_id" ref="hr_timesheet_time_type.view_project_time_type_form"/>
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="customer_billable"/>
                <field name="paid_employee"/>
            </field> 
        </field>
    </record>

    <record id="view_project_time_type_tree" model="ir.ui.view">
        <field name="model">project.time.type</field>
        <field name="inherit_id" ref="hr_timesheet_time_type.view_project_time_type_tree"/>
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="customer_billable"/>
                <field name="paid_employee"/>
            </field>
        </field>
    </record>

</odoo>
