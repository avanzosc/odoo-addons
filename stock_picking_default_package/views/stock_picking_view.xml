<odoo>
  <data>
    <record id="stock_picking_form" model="ir.ui.view">
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock_picking_sorted.sorted_picking_view"/>
        <field name="arch" type="xml">
            <xpath expr="/form/sheet/notebook/page[2]/field[@name='move_ids_without_package']/tree/field[@name='categ_id']" position="after">
                <field name="default_packaging_id"/>
            </xpath>
        </field>
    </record>
  </data>
</odoo>
