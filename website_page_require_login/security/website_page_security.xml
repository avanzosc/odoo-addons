<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="website_page_login_public" model="ir.rule"
                forcecreate="True">
            <field name="name">Hide Web page from Public</field>
            <field name="model_id" ref="website.model_website_page"/>
            <field name="domain_force">
[('website_require_login', '=', False)]
            </field>
            <field name="groups"
                   eval="[(4, ref('base.group_public'))]"/>
        </record>
    </data>
</odoo>
