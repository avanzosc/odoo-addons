<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <record id="stock_by_breeding_wizard_view_form" model="ir.ui.view">
        <field name="name">stock.by.breeding.wizard.form</field>
        <field name="model">stock.by.breeding.wizard</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="meat_cost"/>
                        <field name="date"/>
                    </group>
                    <footer>
                        <button name="button_create_report" string="Print Report" type="object" class="btn-primary"/>
                        or
                        <button special="cancel" string="Cancel" class="oe_link"/>
                    </footer>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
