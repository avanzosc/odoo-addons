<?xml version="1.0" encoding="UTF-8"?>
<odoo>


    <record id="res_area_specialization_tree_view" model="ir.ui.view">
        <field name="name">res.area.specialization.tree.view</field>
        <field name="model">res.area.specialization</field>
        <field name="arch" type="xml">
            <tree string="Area">
                <field name="name"/>
                <field name="description"/>
                <field name="opportunity_space_id"/>
            </tree>
        </field>
    </record>

    <record id="res_area_specialization_search_view" model="ir.ui.view">
        <field name="name">res.area.specialization.search</field>
        <field name="model">res.area.specialization</field>
        <field name="arch" type="xml">
            <search string="Search area">
                <field name="name" />
                <field name="opportunity_space_id"/>
            </search>
        </field>
    </record>

    <record id="res_area_specialization_action" model="ir.actions.act_window">
        <field name="name">Area Specialization</field>
        <field name="res_model">res.area.specialization</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{
        }</field>
    </record>
</odoo>
