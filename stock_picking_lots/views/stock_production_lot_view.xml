<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="stock_production_lot_imei_tree_view" model="ir.ui.view">
        <field name="model">stock.production.lot</field>
        <field name="inherit_id"
               ref="stock.view_production_lot_tree" />
        <field name="arch" type="xml">
        <field name="product_id" position="after">
                <field name="imei"/>
            </field>

        </field>
    </record>

    <record id="stock_production_lot_imei_form_view" model="ir.ui.view">
        <field name="model">stock.production.lot</field>
        <field name="inherit_id"
               ref="stock.view_production_lot_form" />
        <field name="arch" type="xml">
        <field name="product_id" position="after">
                <field name="imei"/>
            </field>

        </field>
    </record>


</odoo>
