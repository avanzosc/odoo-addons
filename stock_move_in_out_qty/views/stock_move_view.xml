<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <record id="view_move_tree" model="ir.ui.view">
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_tree"/>
        <field name="arch" type="xml">
            <field name="product_uom" position="before">
                <field name="origin_qty" optional="show" sum="Origin Total Qty"/>
                <field name="dest_qty" optional="show" sum="Dest Total Qty"/>
                <field name="origin_amount" optional="show" sum="Origin Total Amount"/>
                <field name="dest_amount" optional="show" sum="Dest Total Amount"/>
            </field>
        </field>
    </record>
</odoo>
