<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record model="ir.ui.view" id="wiz_event_attendance_report_form_view">
        <field name="model">wiz.event.attendance.report</field>
        <field
      name="inherit_id"
      ref="event_attendance_report.wiz_event_attendance_report_form_view"
    />
        <field name="arch" type="xml">
            <field name="allowed_customer_ids" position="before">
                <field name="academic_year_id" />
            </field>
            <xpath
        expr="//field[@name='wizard_line_ids']//field[@name='event_id']"
        position="before"
      >
                <field name="academic_year_id" readonly="1" />
            </xpath>
        </field>
    </record>
</odoo>
