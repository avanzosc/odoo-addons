<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="calendar_user_input_view_form" model="ir.ui.view">
        <field name="model">calendar.user_input</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="event_id" />
                    <field name="survey_ids" />
                    <field name="partner_ids" />
                </group>
                <footer>
                    <button name="create_survey_response"
                            string="Create Response" type="object"
                            class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
