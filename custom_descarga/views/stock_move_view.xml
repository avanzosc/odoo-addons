<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_move_tree" model="ir.ui.view">
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_tree" />
        <field name="arch" type="xml">
            <field name="product_uom_qty" position="before">
                <field
          name="download_unit"
          optional="show"
          sum="Total Units"
          string="Units"
          force_save="1"
        />
                <field name="reserved_availability" optional="show" />
            </field>
        </field>
    </record>

    <record id="view_picking_move_tree_inherited" model="ir.ui.view">
        <field name="model">stock.move</field>
        <field
      name="inherit_id"
      ref="stock_picking_batch.view_picking_move_tree_inherited"
    />
        <field name="arch" type="xml">
            <field name="product_uom_qty" position="before">
                <field
          name="download_unit"
          optional="show"
          sum="Total Units"
          string="Units"
          force_save="1"
        />
            </field>
        </field>
    </record>

    <record id="view_move_tree_receipt_picking" model="ir.ui.view">
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_tree_receipt_picking" />
        <field name="arch" type="xml">
            <field name="product_uom_qty" position="before">
                <field name="download_unit" optional="show" force_save="1" />
            </field>
        </field>
    </record>
</odoo>
