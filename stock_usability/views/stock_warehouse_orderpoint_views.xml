<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_warehouse_orderpoint_tree_editable" model="ir.ui.view">
        <field name="model">stock.warehouse.orderpoint</field>
        <field name="inherit_id" ref="stock.view_warehouse_orderpoint_tree_editable" />
        <field name="arch" type="xml">
            <field name="qty_forecast" position="after">
                <field name="qty_available" optional="show" />
                <field name="incoming_qty" optional="show" />
                <field name="outgoing_qty" optional="show" />
                <field name="incoming_qty2" optional="show" />
                <field name="outgoing_qty2" optional="show" />
                <field name="future_virtual_available" optional="show" />
            </field>
        </field>
    </record>
</odoo>
