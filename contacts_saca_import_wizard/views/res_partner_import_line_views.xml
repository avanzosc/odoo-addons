<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="res_partner_import_line_view_form" model="ir.ui.view">
        <field name="model">res.partner.import.line</field>
        <field
      name="inherit_id"
      ref="contacts_import_wizard.res_partner_import_line_view_form"
    />
        <field name="arch" type="xml">
            <field name="partner_website" position="after">
                <field name="partner_farm_numexp" />
                <field name="partner_distance" />
            </field>
        </field>
    </record>

    <record id="res_partner_import_line_view_tree" model="ir.ui.view">
        <field name="model">res.partner.import.line</field>
        <field
      name="inherit_id"
      ref="contacts_import_wizard.res_partner_import_line_view_tree"
    />
        <field name="arch" type="xml">
            <field name="partner_website" position="after">
                <field name="partner_farm_numexp" />
                <field name="partner_distance" />
            </field>
        </field>
    </record>

</odoo>
