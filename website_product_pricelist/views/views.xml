<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="website_sale_pricelist_tree_view" model="ir.ui.view">
        <field name="model">product.pricelist</field>
        <field name="inherit_id" ref="website_sale.website_sale_pricelist_tree_view" />
        <field name="arch" type="xml">
            <field name="website_id" position="after">
                <field name="show_price_website" />
            </field>
        </field>
    </record>
    <record id="website_sale_pricelist_form_view" model="ir.ui.view">
        <field name="model">product.pricelist</field>
        <field name="inherit_id" ref="website_sale.website_sale_pricelist_form_view" />
        <field name="arch" type="xml">
            <field name="selectable" position="after">
                <field name="show_price_website" />
            </field>
        </field>
    </record>
</odoo>
