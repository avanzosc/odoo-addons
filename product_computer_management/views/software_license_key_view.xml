<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="software_license_key_view_tree" model="ir.ui.view">
        <field name="name">software.license.key.tree</field>
        <field name="model">software.license.key</field>
        <field name="arch" type="xml">
            <tree string="Software License Key" editable="top">
                <field name="name" />
            </tree>
        </field>
    </record>

    <record id="software_license_key_action" model="ir.actions.act_window">
        <field name="name">Software License Key</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">software.license.key</field>
        <field name="view_mode">tree</field>
    </record>

    <menuitem
    id="software_license_key_conf"
    name="Software License Key"
    action="software_license_key_action"
    parent="computer_management_conf"
    sequence="11"
  />

</odoo>
