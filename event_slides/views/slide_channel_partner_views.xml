<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="slide_channel_partner_view_tree">
        <field name="model">slide.channel.partner</field>
        <field name="inherit_id" ref="website_slides.slide_channel_partner_view_tree"/>
        <field name="arch" type="xml">
            <field name="partner_email" position="after">
                <field name="event_registration_id" readonly="1"/>
                <field name="real_date_start" />
                <field name="date_start" />
                <field name="real_date_end" />
                <field name="date_end" />
                <field name="birthdate" />
                <field name="age" />
            </field>
        </field>
    </record>
</odoo>
