<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="qc_test_tree_view_inh_qualitycontroclaim" model="ir.ui.view">
        <field name="name">qc.test.tree.view.inh.qualitycontroclaim</field>
        <field name="model">qc.test</field>
        <field name="inherit_id" ref="quality_control_oca.qc_test_tree_view" />
        <field name="arch" type="xml">
            <field name="company_id" position="after">
                <field name="automatic_claims" />
                <field name="automatic_claims_by_line" />
            </field>
        </field>
    </record>
    <record id="qc_test_form_view_inh_qualitycontroclaim" model="ir.ui.view">
        <field name="name">qc.test.form.view.inh.qualitycontroclaim</field>
        <field name="model">qc.test</field>
        <field name="inherit_id" ref="quality_control_oca.qc_test_form_view" />
        <field name="arch" type="xml">
            <field name="active" position="after">
                <field name="automatic_claims" />
                <field name="automatic_claims_by_line" />
            </field>
        </field>
    </record>
</odoo>
