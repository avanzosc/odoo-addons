<?xml version="1.0" encoding="utf-8"?>
    <odoo>
        <!-- VISTA DE FORMULARIO -->
        <record id="partner_form_view_inh_res_partner" model="ir.ui.view">
            <field name="name">partner.form.view.inh.res.partner</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <page name="internal_notes" position="after">
                        <page name="general_data" string="General data">
                                <label for="high_date" string="High date"/>
                                <field name="high_date"/>
                                <p></p>
                                <label for="low_date" string="Low date"/>
                                <field name="low_date"/>
                            <group name="company_type" colspan="4" col="8">
                                <field name="asociated" colspan="4"/>
                                <field name="asociated_type" colspan="4"/>
                                <field name="nature_entity" colspan="4"/>
                                <field name="SME" colspan="4"/>
                                <field name="nature_of_sector" colspan="4"/>
                                <field name="group_of_control" colspan="4"/>
                                <field name="doyou" colspan="4"/>
                                <field name="number_of_employees" colspan="4"/>
                            </group>
                            <group colspan="4">
                                <group name="business_interests">
                                    <field name="country_id" />
                                </group>
                                <group name="external_implantations">
                                    <field name="comertial_implantation" />
                                    <field name="productive_implantation" />
                                </group>
                            </group>
                        </page>
                        <page name="economic_data" string="Economic data" colspan="4" col="8">
                            <group colspan="4">
                                <group>
                                    <field name="date"/>
                                    <field name="real_total_turnover"/>
                                    <field name="real_number_employees"/>
                                    <field name="real_external_billing"/>
                                    <field name="real_external_employees_number"/>
                                    <field name="real_investment_RD"/>
                                </group>
                                <group>
                                    <field name="expected_total_billing"/>
                                    <field name="expected_total_employees_number"/>
                                    <field name="expected_external_billing"/>
                                    <field name="expected_external_employees_number"/>
                                    <field name="expected_investment_RD"/>
                                </group>
                            </group>
                        </page>
                    </page>
            </field>
        </record>
</odoo>