<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record model="ir.module.category" id="module_category_custom_gaia">
            <field name="name">Custom Gaia</field>
        </record>

        <record id="group_custom_gaia_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="module_category_custom_gaia"/>
        </record>

        <record id="group_custom_gaia_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="module_category_custom_gaia"/>
            <field name="implied_ids" eval="[(4, ref('group_custom_gaia_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
        </record>

    </data>
</openerp>