<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="employee_skill_view_tree" model="ir.ui.view">
        <field name="model">hr.skill</field>
        <field name="inherit_id" ref="hr_skills.employee_skill_view_tree"/>
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="code" />
            </field>
        </field>
    </record>
    <record id="employee_skill_view_form" model="ir.ui.view">
        <field name="model">hr.skill</field>
        <field name="inherit_id" ref="hr_skills.hr_skill_view_form"/>
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="code" />
            </field>
        </field>
    </record>
</odoo>
