<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <record id="batch_house_change_view_form" model="ir.ui.view">
        <field name="name">batch.house.change.wizard.form</field>
        <field name="model">batch.house.change.wizard</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="type_id"/>
                        <field name="date_done"/>
                    </group>
                    <footer>
                        <button name="button_change_house" string="Accept"
                                    type="object" class="oe_highlight" />
                        or
                        <button special="cancel" string="Cancel" class="oe_link" />
                    </footer>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
