<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <record id="view_stock_move_line_detailed_operation_tree" model="ir.ui.view">
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_stock_move_line_detailed_operation_tree"/>
        <field name="arch" type="xml">
            <field name="owner_id" position="after">
                <field name="batch_id" string="Mother" attrs="{'column_invisible':[('parent.is_incubator', '=', False)]}"/>
            </field>
            <field name="product_uom_qty" position="after">
                <field name="broken" attrs="{'column_invisible':[('parent.is_incubator', '=', False)]}"/>
                <field name="waste" attrs="{'column_invisible':[('parent.is_incubator', '=', False)]}"/>
            </field>
        </field>
    </record>
</odoo>
