<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <record id="view_picking_form" model="ir.ui.view">
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <field name="is_locked" position="after">
                <field name="is_incubator" invisible="1"/>
            </field>
        </field>
    </record>

    <record id="action_picking_view" model="ir.actions.act_window">
        <field name="name">Picking</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">stock.picking</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem action="action_picking_view" name="Picking" id="menu_picking_incubator" parent="custom_incubator.incubator" sequence="3"/>

</odoo>
