<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_warehouse_orderpoint_tree_editable_config" model="ir.ui.view">
        <field name="model">stock.warehouse.orderpoint</field>
        <field name="inherit_id" ref="stock.view_warehouse_orderpoint_tree_editable_config" />
        <field name="arch" type="xml">
            <field name="active" position="after">
                <field name="name" readonly="1" />
            </field>
            <field name="qty_multiple" positon="after">
                <field name="qty_to_order" readonly="1" optional="hide" />
            </field>
            <field name="json_lead_days_popover" position="after">
                <field name="qty_on_hand" readonly="1" optional="hide" />
                <field name="qty_forecast" readonly="1" optional="hide" />
            </field>
        </field>
    </record>
</odoo>
