<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="miling_list_sync_cron" model="ir.cron" forcecreate="True">
        <field name="name">Sync Mailing List</field>
        <field name="active" eval="True"/>
        <field name="model_id" ref="mass_mailing.model_mail_mass_mailing_list"/>
        <field name="state">code</field>
        <field name="code">model._mailing_list_automatic_sync()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="nextcall" eval="(datetime.now() + timedelta(days=1)).strftime('%Y-%m-%d 01:00:00')" />
    </record>
</odoo>
