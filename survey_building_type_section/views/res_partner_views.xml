<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_partner_form" model="ir.ui.view">
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="res_partner_building_type_section.view_partner_form" />
        <field name="arch" type="xml">
            <field name="service_start_date" position="after">
                <field name="file_number"/>
            </field>
        </field>
    </record>

    <record id="view_partner_tree" model="ir.ui.view">
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="res_partner_building_type_section.view_partner_tree" />
        <field name="arch" type="xml">
            <field name="service_start_date" position="after">
                <field name="file_number" optional="show"/>
            </field>
        </field>
    </record>

    <record id="view_res_partner_filter" model="ir.ui.view">
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="res_partner_building_type_section.view_res_partner_filter" />
        <field name="arch" type="xml">
            <field name="service_start_date" position="after">
                <field name="file_number"/>
            </field>
        </field>
    </record>
</odoo>
