<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="product_informative_location_precision1_view_tree" model="ir.ui.view">
        <field name="model">product.informative.location.precision1</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="description" />
            </tree>
        </field>
    </record>
    <record id="product_informative_location_precision1_view_search" model="ir.ui.view">
        <field name="model">product.informative.location.precision1</field>
        <field name="arch" type="xml">
            <search>
                <field name="name" />
                <field name="description" />
            </search>
        </field>
    </record>
    <record id="product_informative_location_precision1_view_form" model="ir.ui.view">
        <field name="model">product.informative.location.precision1</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <div class="oe_title">
                        <label for="name" />
                        <h1>
                            <field name="name" required="1"/>
                        </h1>
                    </div>
                    <group name="group1" colspan="4">
                        <field name="description" colspan="4" required="1"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record model="ir.actions.act_window" id="action_product_informative_location_precision1">
        <field name="name">Informative location precision 1</field>
        <field name="res_model">product.informative.location.precision1</field>
        <field name="view_type">form</field>
        <field name="view_mode">form,tree</field>
        <field name="view_id" ref="product_informative_location_precision1_view_tree" />
    </record>
    <menuitem id="product_informative_location_precision1_group_view" name="Informative location precision 1"
            action="action_product_informative_location_precision1"
            parent="stock.menu_product_in_config_stock"
            groups="base.group_system"
            sequence="101"/>
</odoo>
